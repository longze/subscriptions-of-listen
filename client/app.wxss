/**app.wxss**/
div,
section {
  display: block;
}

.black {
  color: aquamarine;
}

/* iconfont */
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-star:before { content: "\e627"; }

.icon-jiantou:before { content: "\e66b"; }

.icon-local:before { content: "\e637"; }

.icon-search:before { content: "\e66f"; }

.icon-join:before { content: "\e614"; }

.icon-right-arrows:before { content: "\e731"; }

.icon-coffee:before { content: "\e664"; }

.icon-list:before { content: "\e616"; }

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

/* 超出省略 */
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
