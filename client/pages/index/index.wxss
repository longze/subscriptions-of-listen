.banner .search-section {
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
  width: 100%;
}

.banner .search-section .icon-search {
  position: absolute;
  z-index: 10;
  top: 35rpx;
  right: 35rpx;
  font-size: 42rpx;
  font-weight: 700;
  color: #fff;
}

/*==== 位置信息模块 ====*/
.lbs {
  display: block;
  box-sizing: border-box;
  width: 100%;
  padding: 18px;
  text-align: left;
  color: #555555;
  font-size: 18px;
}

/*==== 导航 ====*/
.nav {
  padding-bottom: 15px;
  border-bottom: 1px solid #f1f1f1;
  color: #999;
  font-family: 'PingFangSC-Regular';
  font-size: 15px;
  word-break: keep-all;
}

.nav .item {
  padding: 15px;
}

.nav .actived {
  color: #000;
}

/*==== 列表块 ====*/
.stager-list {
  display: block;
  box-sizing: border-box;
  width: 100%;
  padding: 0 1em;
}

.stager-item {
  display: flex;
  padding-bottom: 15px;
  border-bottom: 1px solid #f1f1f1;
}

.stager-item:last-child {
  border: none;
}

.stager-item .header-img {
  flex: 0 0 56px;
  width: 56px;
  height: 56px;
  border-radius: 56px;
  margin-top: 15px;
  border: 1px solid #E0E0E0;
}

.stager-item section {
  flex: 1;
  padding-left: 8px;
  overflow: hidden;
}

.stager-item div {
  display: block;
  font-size: 14px;
  line-height: 1em;
}

.stager-item .title {
  padding-top: 18px;
  font-size: 18px;
}

.stager-item .name-line {
  color: #555555;
  padding-top: 10px;
}

.stager-item .address-line {
  color: #999999;
  padding: 10px 0 15px;
}

.stager-item .appointment-line {
  line-height: 28px;
}

.stager-item .appointment {
  display: inline-block;
  background: #F5F5F5;
  border-radius: 3px;
  padding: 0 10px;
}

.stager-item .appointment span {
  color: #fe6b0f;
}

.stager-item .num {
  float: right;
  font-size: 12px;
  color: #999999;
}

/*==== 底部按钮 ====*/


.bottom-bar .iconfont {
  font-size: 18px;
}

.icon-iconset {
  display: inline-block;
  width: 18px;
  height: 18px;
  margin-right: 10px;
}

.to-data {
  display: block;
  padding-left: 21px;
  color: #999;
  font-weight: bold;
  font-size: 13px;
  line-height: 13px;
  height: 13px;
}
.plaing >.iconfont{
  color: #FC6c04
}
.plaing > .data-name{
  color: #FC6c04
}
.today-data {
  padding-left: 21px;
  color: #000;
  font-weight: bold;
  font-size: 34px;
  line-height: 34px;
  margin-top: 15px;
}
.data-warp {
  margin-top: 24px;
}
.one-data{
  padding-left: 17px;
  color: #555;
  font-size: 16px;
}
.one-data {
  height: 18px;
  display: flex;
  flex-direction: row;
  margin-top: 18px;
}
.data-name{
  display: block;
  line-height: 18px;
  font-size: 16px;
}
.iconfont {
  font-size: 18px;
  color: #999;
  margin-right: 8px;
  line-height: 18px;
}
.kipt {
  display: block;
  width: 100%;
  height: 14px;
}
.bottom-bar {
  position: fixed;
  bottom: 0;
  width: 100%;
  border-bottom: 1px solid #E0E0E0;
  box-shadow: 0 -1px 6px 0 rgba(0, 0, 0, 0.20);
  background: #fff;
  font-size: 16px;
  box-sizing: border-box;
  color: #555555;
  padding: 15px 0px 15px 30px;
}
.bottom-title{
  font-size: 16px;
  color: #666;
  font-weight: bold;
  height: 16px;
  line-height: 16px;
  letter-spacing: 2px;
}
.bottom-time {
  font-size: 13px;
  color: #999;
  height: 13px;
  margin-top: 8px;
  line-height: 13px;
  letter-spacing: 1px;
}
.bottom-bar > .iconfont{
  position: absolute;
  top: 25px;
  right: 17px;
  margin-right: 0px;
  font-size: 38px;
}
.hr {
  width: 90%;
  background: rgba(0, 0, 0, 0.20);
  height: 1px;
  margin: 30px auto;
}
.icon-bofang {
  position: absolute;
  display: block;
  top: 15px;
  right: 30px;
  margin-right: 0px;
  width: 38px;
  height: 38px
}